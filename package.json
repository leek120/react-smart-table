{
  "name": "react-smart-table",
  "version": "0.0.1",
  "description": "A smart table component",
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "files": [
    "*.md",
    "demo",
    "es6",
    "lib",
    "umd",
    "src"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6 && rimraf umd && npm run build-umd && npm run build-min",
    "build-cjs": "rimraf lib && babel ./src -d lib",
    "build-es6": "rimraf es6 && babel ./src -d es6 --blacklist=es6.modules",
    "build-umd": "NODE_ENV=development webpack src/index.js umd/Recharts.js",
    "build-min": "NODE_ENV=production webpack src/index.js umd/Recharts.min.js",
    "demo": "webpack-dev-server  --progress --port 3000  --colors --content-base demo --hot --inline --config demo/webpack.config.js",
    "test": "karma start test/karma.conf.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recharts/react-smart-table.git"
  },
  "keywords": [
    "react",
    "table",
    "react-component",
    "reactjs"
  ],
  "author": "xile611",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^2.2.2",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "escope": "^3.6.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "estraverse-fb": "^1.3.1",
    "isparta": "^4.0.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-addons-transition-group": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.1.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-bundle-size-analyzer": "^2.0.1",
    "webpack-dev-server": "^1.14.0"
  },
  "bugs": {
    "url": "https://github.com/recharts/react-smart-table/issues"
  },
  "homepage": "https://github.com/recharts/react-smart-table#readme"
